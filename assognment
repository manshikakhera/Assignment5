c
Copy code
#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 1; i <= N; i++) {
        printf("%d ", 2 * i - 1);
    }
    
    return 0;
}
Print the first N odd natural numbers in reverse order:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 2 * N - 1; i >= 1; i -= 2) {
        printf("%d ", i);
    }
    
    return 0;
}
Print the first N even natural numbers:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 2; i <= 2 * N; i += 2) {
        printf("%d ", i);
    }
    
    return 0;
}
Print the first N even natural numbers in reverse order:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 2 * N; i >= 2; i -= 2) {
        printf("%d ", i);
    }
    
    return 0;
}
Print squares of the first N natural numbers:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 1; i <= N; i++) {
        printf("%d ", i * i);
    }
    
    return 0;
}
Print cubes of the first N natural numbers:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 1; i <= N; i++) {
        printf("%d ", i * i * i);
    }
    
    return 0;
}
Print a table of N:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", N, i, N * i);
    }
    
    return 0;
}
