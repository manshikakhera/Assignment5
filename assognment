int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 0; i < N; i++) {
        printf("MySirG ");
    }
    
    return 0;
}
Print the first N natural numbers:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = 1; i <= N; i++) {
        printf("%d ", i);
    }
    
    return 0;
}
Print the first N natural numbers in reverse order:

#include <stdio.h>

int main() {
    int N;
    printf("Enter the value of N: ");
    scanf("%d", &N);
    
    for (int i = N; i >= 1; i--) {
        printf("%d ", i);
    }
    
    return 0;
}
